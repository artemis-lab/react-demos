{
  "name": "react-demos",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "artemis-lab",
  "license": "MIT",
  "description": "A collection of React demo components showcasing various patterns and techniques",
  "repository": {
    "type": "git",
    "url": "https://github.com/artemis-lab/react-demos.git"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rm -rf dist",
    "dev": "vite",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.18",
    "lucide-react": "0.561.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "7.10.1",
    "tailwindcss": "4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "9.39.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.25",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0",
    "vite": "7.3.0"
  }
}
